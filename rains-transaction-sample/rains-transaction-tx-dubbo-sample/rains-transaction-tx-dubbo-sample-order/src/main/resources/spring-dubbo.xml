<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="dubbo-order"/>


    <dubbo:registry protocol="zookeeper" address="172.26.223.27:2181"/>

    <dubbo:protocol accesslog="true" name="dubbo" port="-1"/>


    <dubbo:service interface="com.happylifeplat.transaction.tx.dubbo.sample.order.api.service.OrderService"
                   ref="orderService"/>

    <dubbo:reference interface="com.happylifeplat.transaction.remote.service.TxManagerRemoteService" id="txManagerRemoteService" />

</beans>